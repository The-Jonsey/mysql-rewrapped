{
  "name": "mysql-rewrapped",
  "version": "1.4.1",
  "description": "A wrapper for MySQL on node to make querying a database as easy as possible",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "./node_modules/mocha/bin/_mocha test/index.js --exit",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-jonsey/mysql-rewrapped.git"
  },
  "keywords": [
    "MySQL",
    "SQL",
    "wrapper",
    "mysql-wrapper"
  ],
  "author": "jonsey",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/the-jonsey/mysql-rewrapped/issues"
  },
  "homepage": "https://github.com/the-jonsey/mysql-rewrapped#readme",
  "dependencies": {
    "mysql": "^2.16.0"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.6",
    "assertthat": "^3.0.0",
    "mocha": "^10.2.0",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2"
  }
}
